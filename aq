SELECT QUEUE_TABLE, PAYLOAD_TYPE
FROM ALL_QUEUE_TABLES
WHERE QUEUE_TABLE = 'YOUR_QUEUE_TABLE_NAME';


SELECT 
    MSGID,
    ENQ_TIME,
    DEQ_TIME,
    TREAT(USER_DATA AS YOUR_SCHEMA.MY_PAYLOAD_TYPE) AS USER_DATA
FROM 
    AQ$YOUR_QUEUE_TABLE_NAME;


SELECT QUEUE_TABLE, PAYLOAD_TYPE
FROM ALL_QUEUE_TABLES
WHERE QUEUE_TABLE = 'YOUR_QUEUE_TABLE_NAME';
-- Replace 'YOUR_QUEUE_TABLE_NAME' with your actual queue table name
-- Replace 'YOUR_PAYLOAD_TYPE' with the actual payload type identified in the previous step

SELECT 
    MSGID,
    ENQ_TIME,
    DEQ_TIME,
    USER_DATA.THE(*) AS USER_DATA
FROM 
    AQ$YOUR_QUEUE_TABLE_NAME;


SELECT 
    MSGID,            -- Adjust based on the actual column name from the DESC output
    ENQ_TIME,         -- Adjust based on the actual column name from the DESC output
    DEQ_TIME,         -- Adjust based on the actual column name from the DESC output
    TREAT(USER_DATA AS TQ_MESSAGE_TYPE) AS USER_DATA
FROM 
    AQ$MY_QUEUE_TABLE;
